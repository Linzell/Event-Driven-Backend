meta {
  name: Upload Prescription
  type: http
  seq: 4
}

put {
  url: http://localhost:4566/dispensary-local-prescriptions/prescriptions/{{dispenseId}}/prescription.jpg
  body: multipartForm
  auth: none
}

headers {
  Content-Type: image/jpeg
}

body:multipart-form {
  file: @file(../assets/ok.jpg)
}

script:post-response {
  console.log("File uploaded to S3");
  console.log("Analyzer Lambda will be triggered automatically");
  console.log("Path: prescriptions/" + bru.getVar("dispenseId") + "/prescription.jpg");
}
